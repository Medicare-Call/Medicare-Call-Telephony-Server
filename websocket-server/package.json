{
  "name": "twilio-realtime",
  "version": "1.0.0",
  "description": "Twilio real-time server with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp src/twiml.xml dist/",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=local nodemon --watch 'src/**/*' --watch '.env' --ext 'ts,js,xml,env' --exec 'ts-node' src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:webhook": "npm run build && node test-webhook.js",
    "test:webhook:custom": "npm run build && node test-webhook.js --custom",
    "test:stt": "ts-node test-stt-service.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "httpdispatcher": "^2.2.0",
    "node-vad": "^1.1.4",
    "prom-client": "^15.1.3",
    "redis": "^5.6.0",
    "ts-node": "^10.9.2",
    "twilio": "^5.5.2",
    "wav": "^1.0.2",
    "winston": "^3.17.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.3",
    "@types/wav": "^1.0.4",
    "@types/ws": "^8.18.1",
    "nodemon": "^3.1.10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
